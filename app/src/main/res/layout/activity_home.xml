<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.project.cvd.studentmanager.Home">

    <FrameLayout
        android:layout_width="851dp"
        android:layout_height="1142dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="97dp">

    </FrameLayout>

    <LinearLayout
        android:id="@+id/ImageHolder"
        android:layout_width="828dp"
        android:layout_height="1153dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="423dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/hshllogogruen1" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="381dp"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/btnStudents"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="110dp"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/pic2"
            android:onClick="GoToStudents"/>

        <ImageButton
            android:id="@+id/btnStundenplan"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="110dp"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/pic2"
            android:onClick="GoToStundenplan"/>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
